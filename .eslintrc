{
  "parser": "babel-eslint",
  "plugins": [
    "babel",
    "import",
    "react"
  ],
  "env": {
    "browser": true,
    "node": true,
    "mocha": true,
    "es6": true
  },
  "rules": {
    "indent": [
      1,
      2,
      {
        "SwitchCase": 1,
        "ignoredNodes": ["TemplateLiteral > *", "JSXOpeningElement > *"]
      }
    ],
    "react/jsx-curly-spacing": 0,
    "react/jsx-indent-props": 0,
    "react/jsx-indent": [
      1,
      2
    ],
    "jsx-quotes": [
      1,
      "prefer-double"
    ],
    "camelcase": [
      1,
      {
        "properties": "always"
      }
    ],
    "comma-dangle": [
      0,
      "never"
    ],
    "semi": [
      1,
      "always"
    ],
    "quotes": [
      1,
      "single",
      {
        "allowTemplateLiterals": true
      }
    ],
    "strict": [
      1,
      "global"
    ],
    "no-multiple-empty-lines": [
      1,
      {
        "max": 1
      }
    ]
  }
}